
<div class="modal" tabindex="-1" id="login">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title">Rellene los datos</h2>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form id="formulariologin" method="GET" action="/usuarios/inicioSesion">
                    <div class="form-group w-50 mx-auto">
                        <label for="Correo">Correo:</label>
                        <input type="email" class="form-control" id="correolog" name="correo">
                        <span id="ErrorCorreoLog" class="text-danger"></span>
                    </div>
                    <div class="form-group w-50 mx-auto">
                        <label for="password">Contraseña:</label>
                        <input type="password" class="form-control" id="passwordlog" name="password">
                        <span id="visibilidadlog" data-mostrar="false">Mostrar</span>
                    </div>
                    <div class="form-group w-50 mx-auto mt-2">
                        <button type="button" id="Iniciar" class="btn btn-dark">Iniciar sesión</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


