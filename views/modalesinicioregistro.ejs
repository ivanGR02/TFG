<div class="modal" tabindex="-1" id="modalCambioFotoPerfil">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title">Cambio de foto de perfil</h2>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form id="cambiarFotoPerfil" method="POST" action="/usuarios/cambiarFoto" enctype="multipart/form-data">
                    <div class="form-group w-50 mx-auto">
                        <label for="FotoPerfil">Subir archivo PNG (tamaño máximo 5 MB):</label>
                        <input type="file" class="form-control" id="cambioFotoPerfil" name="cambioFotoPerfil" accept=".png">
                        <span id="errorFotoPerfil" class="text-danger"></span>
                    </div>
                    <div class="form-group w-50 mx-auto mt-2">
                        <button type="button" class="btn btn-dark " id="CambioFotoPerfil">Cambiar Foto</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>